
func has<E, F, M>(arr &[]<M>E, elem E, equator F) bool {
  i = 0;
  while (i < arr.len()) {
    if (equator(arr[i], elem)) {
      return true;
    }
    set i = i + 1;
  }
  return false;
}

func has<E, M>(arr &[]<M>E, elem E) bool {
  has(arr, elem, ==)
}

func has<E, F, N, M>(seq &[#N]<M>E, elem E, equator F) bool {
  i = 0;
  while (i < seq.len()) {
    if (equator(seq[i], elem)) {
      return true;
    }
    set i = i + 1;
  }
  return false;
}

func has<E, N, M>(seq &[#N]<M>E, elem E) bool {
  return has(seq, elem, ==);
}
